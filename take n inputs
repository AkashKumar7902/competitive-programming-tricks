// first algorithm
#include <iostream>
using namespace std;
int main(){
    int n=0;
    cin>>n;
    int input;
    for(int i=0;i!=n;++i){
        cin>>input;
        //cin followed by code to process value stored
        //in 'input' as it will be overwritten if another 
        //loop begins!
        } 
     }
 
//new algorithm
#include <iostream>
using namespace std;
int main(){ 
    int no_of_inputs=0;
    cin>>no_of_inputs;
    int input;
    cin>>input;
    int cnt=1;
    while(cin>>input){
        //cin followed by code to process value stored
        //in 'input' as it will be overwritten if another 
        //loop begins!
         if(cnt==no_of_inputs) 
              break; // stop taking value 
         ++cnt; //if above 'if' statement is not executed.
          }
    }
